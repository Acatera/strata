`bits 64
default rel

%include "inc/std.inc"

section .bss
    hStdOut resd 1

section .text
    global _start

_start:
    GetStdHandle(STD_OUTPUT_HANDLE, [hStdOut])
`
    gstr message = "Hello, "`
    WriteConsoleA([hStdOut], message, message.length, 0)`
    
    gstr myName = "Alex"`
    WriteConsoleA([hStdOut], myName, myName.length, 0)

    ExitProcess(0)
`