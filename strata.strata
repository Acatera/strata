%endif

    push rax
    mov rax, szSourceCode
    add r11, rax
    strcpy(ptrBuffer64, r11, r12)

    ; printf(`"[WARN] Unknown token '%s'\r\n"`, ptrBuffer64)
    ; jmp .exit
    pop rax

    NextToken()
