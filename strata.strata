            mov r12w, word [rbx + Token.TokenType]`
        end `

        printf(`"[\#27[92mINFO\#27[0m] Procedure '%s' found\r\n"`, ptrBuffer64)

        multipop rax, rbx, rdx, r10, r11, r12, r13, r14, r15

        PopCallerSavedRegs()
        SkipTokens([dqStatementOpCount])
        NextToken()`
    end 

