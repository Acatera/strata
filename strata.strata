        WriteFile([hndDestFile], ptrBuffer256, rax, dwBytesWritten)

        ; it's now safe to pop the current procedure and its symbol table
        call pop_symbol_table

        multipop rax, rbx, rdx, r11, r12, r13, r14, r15

        PopCallerSavedRegs()
        SkipTokens(1)
        NextToken()`
    end 

