            break
        end `
        PushCallerSavedRegs()
        call write_operator
        PopCallerSavedRegs()
        push rax
        pop rax
        sub rax, 8 `
    end
    `
    add rax, 8
    mov [rax], r15
    inc qword [dqOperatorCount]

.end:    
    PopCalleeSavedRegs()
    ret

write_operator:
    PushCalleeSavedRegs()

    mov rax, [dqOperatorCount]
    dec rax
    mov rdx, qword 8 ; size of pointer
    mul rdx
    mov rdx, pOperatorStack
    add rax, rdx
    mov r15, [rax]`

    if r15w == defOperatorPlus then `
        push rax
        sprintf(ptrBuffer256, `"\tpop rax\r\n\tpop rcx\r\n\tadd rax, rcx\r\n\tpush rax\r\n"`)
        WriteFile([hndDestFile], ptrBuffer256, rax, dwBytesWritten)
        pop rax`
    end 

    if r15w == defOperatorMinus then `
        push rax
        sprintf(ptrBuffer256, `"\tpop rcx\r\n\tpop rax\r\n\tsub rax, rcx\r\n\tpush rax\r\n"`)
        WriteFile([hndDestFile], ptrBuffer256, rax, dwBytesWritten)
        pop rax`
    end 

    if r15w == defOperatorMultiply then  `
        push rax
        sprintf(ptrBuffer256, `"\tpop rax\r\n\tpop rcx\r\n\txor rdx, rdx\r\n\tmul rcx\r\n\tpush rax\r\n"`)
        WriteFile([hndDestFile], ptrBuffer256, rax, dwBytesWritten)
        pop rax`
    end

    if r15w == defOperatorDivide then  `
        push rax
        sprintf(ptrBuffer256, `"\tpop rcx\r\n\tpop rax\r\n\txor rdx, rdx\r\n\tdiv rcx\r\n\tpush rax\r\n"`)
        WriteFile([hndDestFile], ptrBuffer256, rax, dwBytesWritten)
        pop rax`
    end

    if r15w == defOperatorModulo then  `
        push rax
        sprintf(ptrBuffer256, `"\tpop rcx\r\n\tpop rax\r\n\txor rdx, rdx\r\n\tdiv rcx\r\n\tpush rdx\r\n"`)
        WriteFile([hndDestFile], ptrBuffer256, rax, dwBytesWritten)
        pop rax`
    end
    `

    dec qword [dqOperatorCount]

.end:
    PopCalleeSavedRegs()
    ret    

