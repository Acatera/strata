    stack resb 1024

section .text
    global _start



_start:
    InitStandardOutput()

    printf(`"Hello, world!\n"`)
    
    ; eval 2 + 3 * 10 (should be 32)

    push 10
    push OP_PLUS
    push 20
    push OP_MUL
    push 30

    pop rax
    cmp rax, OP_PLUS
    je .push_operator

.push_operator:
    pop rax    

    ExitProcess(0)
